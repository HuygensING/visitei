<ivysettings>
  <settings defaultResolver="chain" />

  <resolvers>
    <filesystem name="local">
      <ivy pattern="${ivy.settings.dir}/repository/local/[module]-[artifact]-[revision].[ext]" />
      <artifact pattern="${ivy.settings.dir}/repository/local/[artifact]-[revision].[ext]" />
    </filesystem>	
    <ibiblio name="central" m2compatible="true" root="http://repo1.maven.org/maven2"/>
    <ibiblio name="sun-repo-2" m2compatible="true" root="http://download.java.net/maven/2/" />
    <ibiblio name="Huygens" m2compatible="true" root="http://maven.huygens.knaw.nl/repository/" />
    <chain name="chain">
      <resolver ref="local"/>
      <resolver ref="central"/>
      <resolver ref="sun-repo-2"/>
      <resolver ref="Huygens"/>
    </chain>
  </resolvers>

</ivysettings>
